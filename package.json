{
    "name": "@armathai/pixi-grid",
    "version": "4.0.1",
    "description": "Pixi Grid System",
    "main": "dist/pixi-grid.cjs.js",
    "module": "dist/pixi-grid.esm.js",
    "browser": "dist/pixi-grid.umd.js",
    "types": "dist/types/index-types.d.ts",
    "scripts": {
        "build": "rollup -c && tsc",
