        "@typescript-eslint/parser": "^4.10.0",
        "eslint": "^7.8.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.1.3",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
